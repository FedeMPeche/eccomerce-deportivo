generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Usuario {
  id        Int     @id @default(autoincrement())
  email     String  @unique
  nombre    String
  password  String
  rol       String  @default("cliente")
  creadoEn  DateTime @default(now())
}
